<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.szmsd.bas.dao.BasCityMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.szmsd.bas.domain.BasCity">
        <id column="id" property="id"/>
        <result column="city_code" property="cityCode"/>
        <result column="city_name" property="cityName"/>
        <result column="city_name_en" property="cityNameEn"/>
        <result column="city_name_ar" property="cityNameAr"/>
        <result column="postcode" property="postcode"/>
        <result column="province_code" property="provinceCode"/>
        <result column="province_name" property="provinceName"/>
        <result column="country_code" property="countryCode"/>
        <result column="country_name" property="countryName"/>
        <result column="country_name_en" property="countryNameEn"/>
        <result column="country_name_ar" property="countryNameAr"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
            id, city_code, city_name,city_name_en,city_name_ar, postcode, province_code, province_name, country_code, country_name, country_name_en, country_name_ar
        </sql>


</mapper>
